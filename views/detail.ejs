<%- include('partials/header', { title: movie.title, user: user }) %>
<h1><%= movie.title %></h1>
<img src="<%= movie.thumbnail %>" alt="<%= movie.title %>" width="300" />
<p><strong>Genre:</strong> <%= movie.genre %></p>
<p><strong>Duration:</strong> <%= movie.duration %></p>
<p><%= movie.synopsis %></p>
<a href="/movie/<%= movie.id %>/play">Play</a>
<%- include('partials/footer') %>
